    "require": {
        "iamthes/useful-functions": "dev-master",
        "mikecao/sparrow": "*",
        "gabordemooij/redbean": "dev-master"
    },
    "autoload": {
    	"classmap": [
    		""
    	]
    },
    "repositories": [
        {
